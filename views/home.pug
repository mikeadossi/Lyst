extends layout

block content
  .todrop_header
    .todrop_header_left
      if !isAuthenticated
        a.home(href='/') Lyst
      else
        a.home(href='/lyst') Lyst
    .todrop_header_right
      if !isAuthenticated
        a.todrop_login(href='/login') log in
        a.todrop_sign_up(href='/signup') sign up
      else  
        a.todrop_sign_up(href='/') log out
  
  #landing_container
    .landing_header Welcome
